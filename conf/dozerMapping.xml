<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <configuration>
        <custom-converters>
            <converter type="utils.URLConverter">
                <class-a>java.net.URL</class-a>
                <class-b>java.net.URL</class-b>
            </converter>
        </custom-converters>
        <copy-by-references>
            <copy-by-reference>java.util.UUID</copy-by-reference>
        </copy-by-references>
        
    </configuration>
    <mapping>
        <class-a>models.AssemblyProfile</class-a>
        <class-b>models.transfer.AssemblyProfileTransfer</class-b>
    </mapping>
    <mapping>
        <class-a>models.WorkingGroupProfile</class-a>
        <class-b>models.transfer.WorkingGroupProfileTransfer</class-b>
    </mapping>
    <mapping map-null="false" map-empty-string="false">
        <class-a map-null="false" map-empty-string="false">models.Assembly</class-a>
        <class-b map-null="false" map-empty-string="false">models.transfer.AssemblySummaryTransfer</class-b>
    </mapping>
     <mapping map-null="false" map-empty-string="false">
        <class-a map-null="false" map-empty-string="false">models.WorkingGroup</class-a>
        <class-b map-null="false" map-empty-string="false">models.transfer.WorkingGroupSummaryTransfer</class-b>
    </mapping>
    <mapping map-null="false" map-empty-string="false">
        <class-a map-null="false" map-empty-string="false">models.Assembly</class-a>
        <class-b map-null="false" map-empty-string="false">models.transfer.AssemblyTransfer</class-b>
    </mapping>
    <mapping map-null="false" map-empty-string="false">
        <class-a map-null="false" map-empty-string="false">models.Component</class-a>
        <class-b map-null="false" map-empty-string="false">models.transfer.ComponentTransfer</class-b>
        <field>
            <a>componentId</a>
            <b>componentId</b>
        </field>
    </mapping> 
    <mapping map-null="false" map-empty-string="false">
        <class-a map-null="false" map-empty-string="false">models.ComponentMilestone</class-a>
        <class-b map-null="false" map-empty-string="false">models.transfer.ComponentMilestoneTransfer</class-b>
        <field>
            <a>componentMilestoneId</a>
            <b>milestoneId</b>
        </field>
    </mapping>  
    <mapping map-null="false" map-empty-string="false">
        <class-a map-null="false" map-empty-string="false">models.CampaignTimelineEdge</class-a>
        <class-b map-null="false" map-empty-string="false">models.transfer.CampaignTimelineEdgeTransfer</class-b>
    </mapping>    
    <mapping map-null="false" map-empty-string="false">
        <class-a map-null="false" map-empty-string="false">models.Resource</class-a>
        <class-b map-null="false" map-empty-string="false">models.transfer.ResourceTransfer</class-b>
    </mapping>
    <mapping map-null="false" map-empty-string="false">
        <class-a map-null="false" map-empty-string="false">models.Campaign</class-a>
        <class-b map-null="false" map-empty-string="false">models.transfer.CampaignTransfer</class-b>
    </mapping>        
    <mapping map-null="false" map-empty-string="false">
        <class-a map-null="false" map-empty-string="false">models.Campaign</class-a>
        <class-b map-null="false" map-empty-string="false">models.transfer.CampaignSummaryTransfer</class-b>
    </mapping>
</mappings>
